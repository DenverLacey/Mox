struct Person { name, age }

extend Person {
    def greet(self, name) {
        println(self.name, "says 'Hello!' to", name)
    }

    def have_birthday(self) {
        self.age = self.age + 1
    }
}

var p = Person("Devner", 22)
println(p)

p.have_birthday()
println(p)

def triple(f) {
    f()
    f()
    f()
}

triple(p.have_birthday)

println(p)

println("p.age == 26:", p.age == 26)

p.greet("Jordan")

